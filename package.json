{
  "name": "ngx-schema-form-widgets-primeng-ultima-app",
  "version": "projects/ngx-schema-form-widgets-primeng-ultima/package.json",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --host 0.0.0.0 --port 4444",
    "build": "ng build",
    "test": "ng test",
    "test:chrome_headless": "ng test --browsers=ChromeHeadless --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod ngx-schema-form-widgets-primeng-ultima",
    "pack:lib": "npm pack dist/ngx-schema-form-widgets-primeng-ultima/",
    "pre-publish:lib": "cp -f README.MD dist/ngx-schema-form-widgets-primeng-ultima/README.MD",
    "publish:lib": "npm run pre-publish:lib && npm publish dist/ngx-schema-form-widgets-primeng-ultima/",
    "deploy:lib": "npm run build:lib && npm run pack:lib && npm run publish:lib",
    "start:lib": "npm run build:lib && npm start",
    "-- NUCLI --": "-- ------ --",
    "styles:watch": "sass --watch --no-source-map src/assets",
    "styles:build": "sass --no-source-map src/assets",
    "build:prod": "ng build -c=production --prod --aot --sourceMap=false --build-optimizer",
    "-- Electron --": "-- ------ --",
    "electron:package.json": "node -e \"var fs = require('fs');fs.writeFileSync('dist/ngx-schema-form-widgets-primeng-ultima-app/package.json', fs.readFileSync('electron-package.json'));\"",
    "electron:main.js": "node -e \"var fs = require('fs');fs.writeFileSync('dist/ngx-schema-form-widgets-primeng-ultima-app/electron-main.js', fs.readFileSync('electron-main.js'));\"",
    "electron:pre-build": "npm run electron:package.json && npm run electron:main.js",
    "electron:build": "ng build --base-href ./ && npm run electron:pre-build",
    "electron:start": "electron dist/ngx-schema-form-widgets-primeng-ultima-app",
    "electron:open": "npm run electron:pre-build && npm run electron:start",
    "electron:run": "npm run electron:build && npm run electron:start",
    "bundle-report": "ng build --prod --stats-json --vendor-chunk true && webpack-bundle-analyzer dist/ngx-schema-form-widgets-primeng-ultima-app/stats.json",
    "bundle-report:open": "webpack-bundle-analyzer dist/ngx-schema-form-widgets-primeng-ultima-app/stats.json",
    "doc": "typedoc --hideGenerator --target ES5 --excludeExternals --includeDeclarations --readme README.MD --name ngx-schema-form-widgets-primeng-ultima-app --module commonjs --out doc",
    "--": "--",
    "build:prod-server": "npm run styles:build && ng build --prod -c=production --aot --sourceMap=false --build-optimizer --base-href ./",
    "electron:windows": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-primeng-ultima-app --platform=win32 --overwrite",
    "electron:darwin": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-primeng-ultima-app --platform=darwin --overwrite",
    "electron:linux": "npm run electron:build && electron-packager dist/ngx-schema-form-widgets-primeng-ultima-app --platform=linux --overwrite",
    "electron:mac": "npm run electron:darwin",
    "update:manifest.json": "node -e \"var fs = require('fs');var package_json=JSON.parse(fs.readFileSync('package.json')||{});var manifest_json=JSON.parse(fs.readFileSync('src/manifest.webmanifest')||{});manifest_json.version=package_json.version;fs.writeFileSync('src/manifest.webmanifest', JSON.stringify(manifest_json, null, 2))\"",
    "-- Manifest App Version sync --": "-- ------ --",
    "_prebuild": "npm run update:manifest.json",
    "_prebuild:prod": "npm run update:manifest.json",
    "_prebuild:prod-server": "npm run update:manifest.json",
    "_preelectron:build": "npm run update:manifest.json",
    "-- pre-build --": "",
    "prebuild": "node update-tsconfig.app.json-files.js && npm run _prebuild",
    "prestart": "node update-tsconfig.app.json-files.js",
    "prebuild:prod-server": "node update-tsconfig.app.json-files.js && npm run _prebuild:prod-server",
    "prebuild:prod": "node update-tsconfig.app.json-files.js && npm run _prebuild:prod",
    "preelectron:build": "node update-tsconfig.app.json-files.js && npm run _preelectron:build",
    "prebundle-report": "node update-tsconfig.app.json-files.js",
    "-- Firebase deploy --": "",
    "firebase:publish": "npm run build:prod-server && firebase deploy",
    "electron:help": "npm run electron-packager --help",
    "prebuild:webcomponent": "node elements/build-elements-pre.js --base-href ./ && node elements/build-elements-clean.js",
    "build:webcomponent": "ng build ngx-schema-form-widgets-primeng-ultima-app-elements --output-hashing=none --sourceMap=false --base-href ./",
    "postbuild:webcomponent": "find ./dist/* -name \"*.scss\" -type f -delete && rm -drf ./dist/*/assets/sass && rm -drf ./dist/*/assets/layout/images && find ./dist/*/assets/layout/css/ ! -name \"*-vois.css\" -type f -delete && find ./dist/*/assets/theme/ ! -name \"*-vois.css\" -type f -delete && node elements/build-elements.js",
    "start:webcomponent": "./node_modules/http-server/bin/http-server dist/ngx-schema-form-widgets-primeng-ultima-app-elements-publish/ --proxy http://localhost:8080?",
    "postinstall": "npm run styles:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.12",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/elements": "9.1.12",
    "@angular/forms": "9.1.12",
    "@angular/localize": "9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@angular/pwa": "0.901.10",
    "@angular/router": "9.1.12",
    "@angular/service-worker": "9.1.12",
    "@fullcalendar/core": "4.0.2",
    "@fullcalendar/daygrid": "4.0.1",
    "@fullcalendar/interaction": "4.0.2",
    "@fullcalendar/timegrid": "4.0.1",
    "@techiediaries/ngx-qrcode": "^9.1.0",
    "@webcomponents/webcomponentsjs": "2.4.4",
    "chart.js": "2.7.3",
    "intl": "1.2.5",
    "jexl": "^2.3.0",
    "moment": "2.20.1",
    "nanoscroller": "^0.8.7",
    "ngx-openlayers": "^1.0.0-next.15",
    "ngx-recaptcha-easy": "9.0.0",
    "ngx-schema-form": "~2.5.20",
    "ngx-schema-form-view": "~1.0.10",
    "ol": "^6.3.1",
    "primeflex": "1.0.0",
    "primeicons": "1.0.0",
    "primeng": "9.0.2",
    "prismjs": "1.15.0",
    "quill": "1.3.2",
    "rxjs": "^6.6.2",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "z-schema": "^4.2.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/compiler-cli": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@types/jasmine": "3.5.7",
    "@types/jasminewd2": "2.0.8",
    "@types/jexl": "^2.1.0",
    "@types/node": "12.12.29",
    "codelyzer": "5.2.1",
    "concat": "latest",
    "electron": "^1.4.13",
    "electron-packager": "^12.2.0",
    "enhanced-resolve": "^3.4.1",
    "fs-extra": "latest",
    "http-server": "latest",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "ng-packagr": "^9.0.0",
    "protractor": "5.4.3",
    "puppeteer": "1.9.0",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typedoc": "^0.8.0",
    "typescript": "3.7.5",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
